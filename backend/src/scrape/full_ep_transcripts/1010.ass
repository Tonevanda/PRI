Dialogue: 0,0:01:45.58,0:01:51.34,Default,,0,0,0,,{\i1}Kick away the darkness{\i0}
Dialogue: 0,0:01:51.42,0:01:54.71,On Top,,0,0,0,,{\i1}bokura wa dokomade mo yukeru{\i0}
Dialogue: 0,0:01:51.42,0:01:54.71,Default,,0,0,0,,{\i1}We'll go wherever we wanna go{\i0}
Dialogue: 0,0:01:54.76,0:01:58.68,On Top,,0,0,0,,{\i1}namida mo mayoi mo isso hikitsurete{\i0}
Dialogue: 0,0:01:54.76,0:01:58.68,Default,,0,0,0,,{\i1}Tears and doubts in tow{\i0}
